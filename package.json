{
  "name": "@rubixstudios/payload-unsend",
  "version": "1.11.5",
  "description": "Payload Unsend Email Adapter",
  "homepage": "https://rubixstudios.com.au",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubix-studios-pty-ltd/payload-unsend.git",
    "directory": "packages/payload-unsend"
  },
  "bugs": {
    "url": "https://github.com/rubix-studios-pty-ltd/payload-unsend/issues"
  },
  "keywords": [
    "payload",
    "payloadcms",
    "email",
    "unsend",
    "adapter",
    "plugin"
  ],
  "license": "MIT",
  "author": "Rubix Studios <hello@rubixstudios.com.au> (https://rubixstudios.com.au)",
  "maintainers": [
    {
      "name": "Rubix Studios",
      "email": "hello@rubixstudios.com.au",
      "url": "https://rubixstudios.com.au"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc-build --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "release": "pnpm version patch && pnpm publish",
    "release:minor": "pnpm version minor && pnpm publish",
    "release:major": "pnpm version major && pnpm publish",
    "clean": "rimraf -g {dist,*.tsbuildinfo}",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "update": "pnpm update && pnpm install && pnpm prune && pnpm dedupe",
    "prepublishOnly": "pnpm clean && pnpm turbo build",
    "test": "jest"
  },
  "devDependencies": {
    "@jest/globals": "30.0.5",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "@swc/jest": "^0.2.39",
    "@types/jest": "30.0.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.4.5",
    "jest": "^30.0.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "payload": "^3.28.1"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts"
  },
  "dependencies": {
    "@payloadcms/eslint-config": "^3.28.0"
  },
  "packageManager": "pnpm@10.14.0"
}
